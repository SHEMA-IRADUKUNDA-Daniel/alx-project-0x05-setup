{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "paths": {
      "@/*": ["../alx-project-0x13/*"]
    }
  },
  "include": [
    "../alx-project-0x13/next-env.d.ts",
    "../alx-project-0x13/**/*.ts",
    "../alx-project-0x13/**/*.tsx"
  ],
  "exclude": ["../alx-project-0x13/node_modules"]
}
